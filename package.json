{
  "name": "prm-gp2gp-dashboard",
  "private": true,
  "description": "GP2GP Service Dashboard",
  "version": "1.0.0",
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy": "gatsby-plugin-s3 deploy",
    "test": "jest",
    "format": "prettier --write \"**/*.{js,jsx,scss,md,tsx}\"",
    "checkFormat": "prettier --check \"**/*.{js,jsx,scss,md,tsx}\"",
    "getPracticeMetrics": "node scripts/get-s3-data.js --datatype practiceMetrics",
    "getNationalMetrics": "node scripts/get-s3-data.js --datatype nationalMetrics",
    "getMetadata": "node scripts/get-s3-data.js --datatype metadata"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run checkFormat && npm test && npm run build"
    }
  },
  "dependencies": {
    "@types/react-helmet": "^6.1.0",
    "aws-sdk": "^2.637.0",
    "classnames": "^2.2.6",
    "date-fns": "^2.16.1",
    "gatsby": "^2.32.3",
    "gatsby-plugin-layout": "^1.1.23",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-s3": "^0.3.5",
    "gatsby-plugin-sass": "^2.1.30",
    "gatsby-plugin-typescript-checker": "^1.1.1",
    "gatsby-source-filesystem": "^2.1.53",
    "gatsby-transformer-json": "^2.2.27",
    "js-search": "^2.0.0",
    "lodash": "^4.17.20",
    "nhsuk-frontend": "^4.0.0",
    "node-sass": "^4.14.1",
    "react": "^16.13.0",
    "react-autosuggest": "^10.0.3",
    "react-cookie": "^4.0.3",
    "react-dom": "^16.13.0",
    "react-helmet": "^6.1.0",
    "react-highlight-words": "^0.16.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^7.29.2",
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^11.2.3",
    "@testing-library/react-hooks": "^4.0.0",
    "@testing-library/user-event": "^12.6.0",
    "@types/jest": "^26.0.15",
    "@types/jest-when": "^2.7.2",
    "@types/moxios": "^0.4.10",
    "@types/react-dom": "^17.0.0",
    "babel-jest": "^25.1.0",
    "babel-preset-gatsby": "^0.2.35",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-when": "^3.2.0",
    "moxios": "^0.4.0",
    "prettier": "1.19.1",
    "react-test-renderer": "^16.13.0",
    "ts-jest": "^26.4.3",
    "typescript": "^4.0.5"
  }
}
